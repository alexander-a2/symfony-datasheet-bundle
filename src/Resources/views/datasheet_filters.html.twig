{% for filter in datasheet.getFilters() %}
    {% for attributeName, attributeType in filter.attributes %}
        <input type="hidden"
               name="{{ datasheet.name }}[{{ datasheet.queryKey('datasheet_filters') }}][{{ filter.shortName }}][{{ attributeName }}]"
               value="{{ filter.getParameter(attributeName) }}"
               autocomplete="off"
               placeholder="{{ filter.shortName }} - {{ attributeName }}"
        >
    {% endfor %}
{% endfor %}